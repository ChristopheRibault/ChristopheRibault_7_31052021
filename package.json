{
  "name": "p7_lespetitsplats",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "sass": "sass --watch ./style/index.sass ./style/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristopheRibault/ChristopheRibault_7_31052021.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChristopheRibault/ChristopheRibault_7_31052021/issues"
  },
  "homepage": "https://github.com/ChristopheRibault/ChristopheRibault_7_31052021#readme",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "globals": {
      "require": true
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ],
      "array-bracket-spacing": [
        "error",
        "always",
        {
          "singleValue": false,
          "objectsInArrays": false,
          "arraysInArrays": false
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": true,
          "objectsInObjects": false
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "max-len": [
        "error",
        {
          "code": 70,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreComments": true
        }
      ]
    },
    "extends": "eslint:recommended"
  },
  "eslintIgnore": [
    "webpack.config.js",
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.7.2",
    "miragejs": "^0.1.41"
  }
}
